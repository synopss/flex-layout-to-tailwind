{
  "name": "flex-layout-to-tailwind",
  "version": "0.0.1",
  "description": "CLI that migrates @angular/flex-layout to tailwindcss",
  "main": "./dist/main.js",
  "bin": {
    "fltt": "./dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node ./src/main.ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{js,ts}",
    "lint:fix": "eslint src/**/*.{js,ts} --fix",
    "prettier": "prettier --check src/**/*.{js,ts,json}",
    "prettier:write": "prettier --write src/**/*.{js,ts,json}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synopss/flex-layout-to-tailwind.git"
  },
  "keywords": [
    "angular",
    "flex",
    "tailwindcss",
    "tailwind",
    "migrate",
    "migration"
  ],
  "author": "synopss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/synopss/flex-layout-to-tailwind/issues"
  },
  "homepage": "https://github.com/synopss/flex-layout-to-tailwind#readme",
  "dependencies": {
    "@inquirer/prompts": "^3.0.2",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.0.0",
    "nanospinner": "^1.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fs-extra": "^11.1.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
